N = int(input())
A = []
C = []
while True:
    try:
        d = input().split(' ')
        for num in d:
            A.append(int(num))
    except:
        break
for i in range(N):
    B = []
    for k in range(len(A)):
        if A[k] == i:
            B.append(A[k+1])
    if B:
        C.append(B)
print(C)
C.sort(key=lambda s: (sum(s), s.sort(reverse=True)), reverse=True)
for i in C:
    for k in i:
        print(k, end=' ')
