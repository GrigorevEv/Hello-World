N = int(input())
d = input()
A = []
C = []

while d != '#':
    A.append(d)
    d = input()

for i in range(N):
    B = []
    for k in range(len(A)):
        f = A[k].split()
        if int(f[0]) == i:
            B.append(int(f[1]))
    if B:
        C.append(B)

C.sort(key=lambda s: (sum(s), s.sort(reverse=True)), reverse=True)
for i in C:
    for k in i:
        print(k, end=' ')
